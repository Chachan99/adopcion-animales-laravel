<svg xmlns="http://www.w3.org/2000/svg" width="400" height="300" viewBox="0 0 400 300">
  <defs>
    <linearGradient id="bgFund" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fef3c7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fde68a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#bgFund)"/>
  
  <!-- Building silhouette -->
  <g transform="translate(200,150)">
    <!-- Main building -->
    <rect x="-50" y="-30" width="100" height="80" fill="#a78bfa" opacity="0.7" rx="5"/>
    
    <!-- Roof -->
    <polygon points="-60,-30 0,-60 60,-30" fill="#8b5cf6" opacity="0.8"/>
    
    <!-- Door -->
    <rect x="-10" y="20" width="20" height="30" fill="#6d28d9" opacity="0.8" rx="10" ry="0"/>
    
    <!-- Windows -->
    <rect x="-35" y="-10" width="15" height="15" fill="#fbbf24" opacity="0.9" rx="2"/>
    <rect x="20" y="-10" width="15" height="15" fill="#fbbf24" opacity="0.9" rx="2"/>
    <rect x="-35" y="10" width="15" height="15" fill="#fbbf24" opacity="0.9" rx="2"/>
    <rect x="20" y="10" width="15" height="15" fill="#fbbf24" opacity="0.9" rx="2"/>
  </g>
  
  <!-- Paw prints decoration -->
  <g transform="translate(80,80)" opacity="0.3">
    <circle cx="0" cy="0" r="3" fill="#8b5cf6"/>
    <circle cx="-5" cy="-8" r="2" fill="#8b5cf6"/>
    <circle cx="5" cy="-8" r="2" fill="#8b5cf6"/>
    <circle cx="-3" cy="-15" r="2" fill="#8b5cf6"/>
    <circle cx="3" cy="-15" r="2" fill="#8b5cf6"/>
  </g>
  
  <g transform="translate(320,200)" opacity="0.3">
    <circle cx="0" cy="0" r="3" fill="#8b5cf6"/>
    <circle cx="-5" cy="-8" r="2" fill="#8b5cf6"/>
    <circle cx="5" cy="-8" r="2" fill="#8b5cf6"/>
    <circle cx="-3" cy="-15" r="2" fill="#8b5cf6"/>
    <circle cx="3" cy="-15" r="2" fill="#8b5cf6"/>
  </g>
  
  <!-- Text -->
  <text x="200" y="220" text-anchor="middle" font-family="Arial, sans-serif" font-size="18" fill="#7c3aed" font-weight="600">
    Logo no disponible
  </text>
  <text x="200" y="245" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#a78bfa">
    Fundación de adopción
  </text>
</svg>